{
    "name": "shengyouai/subsys-ucenter",
    "description": "子系统用户中心",
    "authors": [
        {
            "name": "上海圣有爱科技有限公司",
            "email": "shengyouai@shengyouai.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "illuminate/filesystem": "~5|^6|^7|^8",
        "illuminate/routing": "~5|^6|^7|^8",
        "illuminate/config": "~5|^6|^7|^8",
        "illuminate/support": "~5|^6|^7|^8",
        "illuminate/database": "~5|^6|^7|^8",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shengyouai\\Sub\\UserCenter\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Shengyouai\\Sub\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Shengyouai\\Sub\\UserCenter\\UserCenterServiceProvider"
            ],
            "dont-discover": [

            ]
        },
        "aliases": {
            "UserCenter": "Shengyouai\\Sub\\UserCenter\\UserCenter"
        }
    }
}
